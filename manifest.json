{
  "name": "Price Match",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "反感JD价格保护需要自己时刻关注，并手动申请。所以有了这个自动化申请JD价格保护的chrome插件",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_title": "Price Match",
    "default_icon": "images/icon.png",
    "default_popup": "popup/index.html"
  },
  "web_accessible_resources": [
    "content/index.js"
  ],
  "content_scripts": [{
    "js": ["content/include.js"],
    "matches": [
      "http://*.jd.com/*",
      "https://*.jd.com/*"
    ],
    "run_at": "document_end"
  }],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
